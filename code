                Console.BackgroundColor = ConsoleColor.Gray;
                Console.ForegroundColor = ConsoleColor.Gray;
                Console.Write("0");
                Console.SetCursorPosition(j, 46);
                Console.BackgroundColor = ConsoleColor.Gray;
                Console.ForegroundColor = ConsoleColor.Gray;
                Console.Write("0");
            }
            Console.ResetColor();
            int hh_1 = 38;
            int hh_2 = 50;
            s(2, hh_1);
            t(11, hh_1);
            a(20, hh_1);
            n(33, hh_1);
            d(48, hh_1);
            for (int i = 1; i <= 10; i++)
