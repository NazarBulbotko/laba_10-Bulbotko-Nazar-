        static void Main(string[] args)
        {
            Console.Title = "BlackJack";
            Random rnd = new Random();
            int mast = rnd.Next(1, 5);
            int nom = rnd.Next(2, 15);
            int dmast_1 = mast;
            int dnom_1 = nom;
            if (nom < 11)
            {
                dnom_1 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                dnom_1 = 10;
            }
            else
            {
                dnom_1 = 11;
            }
            cards(10, 1, mast, nom);
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int dmast_2 = mast;
            int dnom_2 = nom;
            if (nom < 11)
            {
                dnom_2 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                dnom_2 = 10;
            }
            else
            {
                dnom_2 = 11;
            }
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int dmast_3 = mast;
            int dnom_3 = nom;
            if (nom < 11)
            {
                dnom_3 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                dnom_3 = 10;
            }
            else
            {
                dnom_3 = 11;
            }
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int dmast_4 = mast;
            int dnom_4 = nom;
            if (nom < 11)
            {
                dnom_4 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                dnom_4 = 10;
            }
            else
            {
                dnom_4 = 11;
            }
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int dmast_5 = mast;
            int dnom_5 = nom;
            if (nom < 11)
            {
                dnom_5 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                dnom_5 = 10;
            }
            else
            {
                dnom_5 = 11;
            }
            int height = 14;
            int width = 21;
            for (int i = 1; i <= width; i++)
            {
                for (int j = 1; j <= height; j++)
                {
                    int r = Math.Abs(j - i);
                    if (r % 2 == 0)
                    {
                        Console.SetCursorPosition((i + 40), (j + 1));
                        Console.BackgroundColor = ConsoleColor.Cyan;
                        Console.ForegroundColor = ConsoleColor.Cyan;
                        Console.Write("0");
                    }
                    else
                    {
                        Console.SetCursorPosition((i + 40), (j + 1));
                        Console.BackgroundColor = ConsoleColor.White;
                        Console.ForegroundColor = ConsoleColor.White;
                        Console.Write("0");
                    }
                }
            }
            Console.ResetColor();
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int pmast_1 = mast;
            int pnom_1 = nom;
            cards(10, 20, mast, nom);
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int pmast_2 = mast;
            int pnom_2 = nom;
            if (nom < 11)
            {
                pnom_2 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                pnom_2 = 10;
            }
            else
            {
                pnom_2 = 11;
            }
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int pmast_3 = mast;
            int pnom_3 = nom;
            if (nom < 11)
            {
                pnom_3 = nom;
            }
            else if (nom >= 11 && nom < 14)
            {
                pnom_3 = 10;
            }
            else
            {
                pnom_3 = 11;
            }
            mast = rnd.Next(1, 5);
            nom = rnd.Next(2, 15);
            int pmast_4 = mast;
