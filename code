                        Console.SetCursorPosition(i + 210, j + 10);
                        Console.Write("0");
                    }
                }
                Console.ResetColor();
                if (rate == 100)
                {
                    num_6(210, -1);
                    num_5(220, -1);
                    num_0(230, -1);
                    num_0(230, 10);
                }
                else if (rate == 50)
                {
                    num_6(210, -1);
                    num_0(220, -1);
                    num_0(230, -1);
                    num_0(230, 10);
                }
            }
            Console.ReadKey();
            }
