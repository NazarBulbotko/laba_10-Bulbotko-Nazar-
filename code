            {
                for (int d = 12; d < width; d++)
                {
                    for (int r = 6; r < height; r++)
                    {
                        if (r == 6 && (d == 15 || d == 16 || d == 17) || r == 7 && (d == 15 || d == 16 || d == 17) ||
