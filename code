            {
                pnom_5 = 10;
            }
            else
            {
                pnom_5 = 11;
            }
            cards(40, 20, mast, nom);
            Console.ResetColor();
            for (int i = 1; i <= 10; i++)
            {
                Console.SetCursorPosition(1, 36 + i);
                Console.BackgroundColor = ConsoleColor.Gray;
                Console.ForegroundColor = ConsoleColor.Gray;
                Console.Write("0");
                Console.SetCursorPosition(58, 36 + i);
                Console.BackgroundColor = ConsoleColor.Gray;
                Console.ForegroundColor = ConsoleColor.Gray;
                Console.Write("0");
